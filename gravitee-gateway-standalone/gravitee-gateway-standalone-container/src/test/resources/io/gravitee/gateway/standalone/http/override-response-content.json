{
  "id": "api-test",
  "name": "api-test",

  "proxy": {
    "context_path": "/api",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/api"
      }
    ],
    "strip_context_path": false,
    "dumpRequest": true
  },

  "paths": {
    "/": [
      {
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "HEAD",
          "PATCH",
          "OPTIONS",
          "TRACE",
          "CONNECT"
        ],
        "override-response-content": {}
      }
    ]
  }
}