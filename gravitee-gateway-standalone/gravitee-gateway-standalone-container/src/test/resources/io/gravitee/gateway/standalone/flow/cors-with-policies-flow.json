{
  "id": "my-api",
  "name": "my-api",
  "gravitee": "2.0.0",
  "proxy": {
    "context_path": "/test",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/team",
        "http": {
          "connectTimeout": 3000,
          "readTimeout": 60000
        }
      }
    ],
    "cors" : {
      "enabled" : true,
      "allowCredentials" : false,
      "allowOrigin" : [ "http://localhost" ],
      "allowHeaders" : [ "x-forwarded-for" ],
      "allowMethods" : [ "DELETE", "POST", "GET", "PATCH" ],
      "exposeHeaders" : [ "x-forwarded-host" ],
      "maxAge" : -1,
      "runPolicies": true
    }
  },
  "flows": [
    {
      "name": "flow-1",
      "methods": [ "OPTIONS" ],
      "path-operator": {
        "path": "/",
        "operator": "STARTS_WITH"
      },
      "pre": [
        {
          "policy": "my-policy",
          "name": "My policy",
          "description": "Step description",
          "configuration": {}
        }
      ],
      "post": [
        {
          "policy": "my-policy",
          "name": "My policy",
          "description": "Step description",
          "configuration": {}
        }
      ]
    }
  ]
}
