{
  "id": "my-api",
  "name": "my-api",
  "gravitee": "2.0.0",
  "proxy": {
    "context_path": "/test",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/team",
        "http": {
          "connectTimeout": 3000,
          "readTimeout": 60000
        }
      }
    ]
  },
  "plans": [
    {
      "id": "plan-1",
      "name": "Plan 1",
      "security": "key_less",
      "flows": [
        {
          "name": "ALL",
          "methods": [
            "GET"
          ],
          "path-operator": {
            "path": "/",
            "operator": "STARTS_WITH"
          },
          "pre": [
            {
              "policy": "my-policy",
              "name": "My policy",
              "description": "Step description",
              "configuration": {}
            }
          ],
          "post": [
            {
              "policy": "my-policy",
              "name": "My policy",
              "description": "Step description",
              "configuration": {}
            }
          ]
        }
      ]
    }
  ]
}
