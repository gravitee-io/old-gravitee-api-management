{
  "id": "api-test",
  "name": "api-test",

  "proxy": {
    "context_path": "/test",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/team"
      }
    ],
    "strip_context_path": false,
    "dumpRequest": true,
    "cors" : {
      "enabled" : true,
      "allowCredentials" : false,
      "allowOrigin" : [ "*" ],
      "allowHeaders" : [ "x-forwarded-for" ],
      "allowMethods" : [ "DELETE", "POST", "GET", "PATCH" ],
      "exposeHeaders" : [ "x-forwarded-host" ],
      "maxAge" : -1
    }
  },

  "paths": {
    "/*": [
    ]
  }
}