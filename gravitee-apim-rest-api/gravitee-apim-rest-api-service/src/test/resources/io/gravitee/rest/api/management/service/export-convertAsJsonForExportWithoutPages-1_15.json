{
  "description" : "Gravitee.io",
  "gravitee": "1.0.0",
  "resources" : [ ],
  "properties" : [ ],
  "flow_mode": "DEFAULT",
  "groups" : [ "My Group" ],
  "members" : [ {
    "username" : "johndoe-sourceId",
    "role" : "PRIMARY_OWNER"
  } ],
  "plans" : [ {
    "id" : "plan-id",
    "description" : "free plan",
    "validation" : "AUTO",
    "security" : "API_KEY",
    "type" : "API",
    "status" : "PUBLISHED",
    "apis" : [ "id-api" ],
    "order" : 0,
    "paths" : {
      "/" : [ {
        "methods" : [ "GET" ],
        "rate-limit" : {
          "rate": {
            "limit": 1,
            "periodTime": 1,
            "periodTimeUnit": "SECONDS"
          }
        },
        "enabled" : true
      } ]
    },
    "comment_required" : false
  } ],
  "metadata":[
    {
      "key":"metadata-key",
      "name":"metadata-name",
      "format":"STRING",
      "value":"metadata-value",
      "defaultValue":"metadata-default-value",
      "apiId":"id-api"
    }
  ],
  "proxy": {
    "context_path": "/test",
    "strip_context_path": false,
    "loggingMode": "CLIENT_PROXY",
    "endpoints": [
      {
        "name": "default",
        "target": "http://test",
        "weight": 1,
        "backup": false,
        "type": "HTTP",
        "http": {
          "connectTimeout": 5000,
          "idleTimeout": 60000,
          "keepAlive": true,
          "readTimeout": 10000,
          "pipelining": false,
          "maxConcurrentConnections": 100,
          "useCompression": true,
          "followRedirects": false
        }
      }
    ],
    "load_balancing": {
      "type": "ROUND_ROBIN"
    }
  }
}
